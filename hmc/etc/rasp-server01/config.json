 {
 "checkgatewaysintervall":40,
 "core":{
	"confFile":{
 		"basePath":"etc/",
 		"filePath":"database/",
 		"defaultEvent":"defaultEvents.json",
 		"devices":"devices.json",
 		"gateways":"gateways.json",
 		"eventHandler":"eventHandler.json",
 		"remoteCore":"coreServer.json"}},
 "multilogger":{
 	"enable":true,
 	"filter":{
 		"loglevel":{
 			"unkown":true
 			},
 		"package":{
 			"unkown":true
 			}
 		},
 	"outputs":{
 		"console":{
 			"enable":true,
 			"package":"console",
 			"config":{
 				"color":{
 						"debug":"32m",
 						"info":"33m",
 						"error":"31m",
 						"warning":"31m",
 						"emergency":"31m",
 						"data":"34m" 						
 					},
 				"filter":{
 					"loglevel":{
 						"unkown":true,
 						"data":false
 						},
 					"package":{
 						"unkown":true
 						}
 					}
 				}
 			},
 		"debug":{
 			"enable":true,
 			"package":"file",
 			"config":{
 				"dir":"log/",
 				"filename":"rasp_server01_debug.log",
 				"filesize":10,
 				"maxfiles":5,
 				"append":false,
 				"filter":{
 					"loglevel":{
 						"unkown":true
 								},
 					"package":{
 						"unkown":true
 						}
 					}
 				}
 			}
 		}
 	}
}